    "@vitejs/plugin-react": "^5.0.4",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
